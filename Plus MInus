static void plusMinus(int [] arr)
 {
        float count1 = 0;
        float count2 = 0;
        float count3 = 0;
 for(int i=0;i<arr.length;i++)
{
    if(arr[i]>0)
    {
        count1++;
    }
    else if(arr[i]<0)
    {
        count2++;
    }
    else if(arr[i]==0)
    {
        count3++;
    }
}
System.out.println(count1/arr.length);
System.out.println(count2/arr.length);
    System.out.println(count3/arr.length);
    }
