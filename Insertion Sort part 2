public class Solution {

    // Complete the insertionSort2 function below.
    static void insertionSort2(int n, int[] arr) {
        for(int i=1;i<n;i++)
        {
            int temp=arr[i];
            for(int j=i-1;j>=0 &&temp<arr[j];j--)
            {
                arr[j+1]=arr[j];
                arr[j]=temp;
            }
            for(int k=0;k<n;k++)
            {
                System.out.print(arr[k]+" ");
            }
                System.out.println();

    }
    }
