public class Solution {

    /*
     * Complete the gradingStudents function below.
     */
    static int[] gradingStudents(int[] grades) {
    for(int i=0;i<grades.length;i++)
    {
        if (grades[i]>=38)
        {
            if(grades[i]%5>2)
            {
                int d=grades[i]/5;
                grades[i]=5*(d+1);
            }
        }
    }

        return grades;
    }
